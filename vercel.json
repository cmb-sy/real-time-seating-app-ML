{
  "version": 2,
  "buildCommand": "chmod +x install_minimal.sh && ./install_minimal.sh",
  "outputDirectory": ".vercel/output",
  "functions": {
    "api/*.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "functions/index.py"
    },
    {
      "src": "/health",
      "dest": "functions/health.py"
    },
    {
      "src": "/api/health",
      "dest": "functions/health.py"
    },
    {
      "src": "/predictions/today-tomorrow",
      "dest": "functions/predictions_today_tomorrow.py"
    },
    {
      "src": "/api/predictions/today-tomorrow",
      "dest": "functions/predictions_today_tomorrow.py"
    },
    {
      "src": "/predictions/weekly-average",
      "dest": "functions/predictions_weekly_average.py"
    },
    {
      "src": "/api/predictions/weekly-average",
      "dest": "functions/predictions_weekly_average.py"
    },
    {
      "src": "/supabase/sync",
      "dest": "functions/supabase_sync.py"
    },
    {
      "src": "/api/supabase/sync",
      "dest": "functions/supabase_sync.py"
    },
    {
      "src": "/ml/predict",
      "dest": "functions/predictions_today_tomorrow.py"
    },
    {
      "src": "/analysis/weekday_analysis",
      "dest": "functions/predictions_today_tomorrow.py"
    }
  ]
}
